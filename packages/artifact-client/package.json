{
  "name": "@templar/artifact-client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Create, discover, and spawn persistent artifacts for Templar agents",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tools": {
      "types": "./dist/tools.d.ts",
      "import": "./dist/tools.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "test": "vitest run"
  },
  "dependencies": {
    "@nexus/sdk": "workspace:*",
    "@templar/core": "workspace:*",
    "@templar/errors": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.2.2"
  }
}
