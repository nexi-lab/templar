{
  "name": "@nexus/sdk",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./agents": {
      "types": "./dist/resources/agents.d.ts",
      "import": "./dist/resources/agents.js"
    },
    "./tools": {
      "types": "./dist/resources/tools.d.ts",
      "import": "./dist/resources/tools.js"
    },
    "./channels": {
      "types": "./dist/resources/channels.d.ts",
      "import": "./dist/resources/channels.js"
    },
    "./memory": {
      "types": "./dist/resources/memory.d.ts",
      "import": "./dist/resources/memory.js"
    },
    "./permissions": {
      "types": "./dist/resources/permissions.d.ts",
      "import": "./dist/resources/permissions.js"
    },
    "./sandbox": {
      "types": "./dist/resources/sandbox.d.ts",
      "import": "./dist/resources/sandbox.js"
    },
    "./http": {
      "types": "./dist/http/index.d.ts",
      "import": "./dist/http/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "test": "vitest run"
  },
  "dependencies": {
    "@templar/errors": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^25.2.2"
  }
}
